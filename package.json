    "form-data": "^2.5.1",
    "is-builtin-module": "^1.0.0",
    "mocha": "^5.0.0",
    "nyc": "11.9.0",
    "parted": "^0.1.1",
    "promise": "^8.0.3",
    "resumer": "0.0.0",
    "rollup": "^0.63.4",
